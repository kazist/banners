    "query": {
        "from": {
            "table": "Banners\\Banners\\Options\\Code\\Tables\\Options",
            "alias": "bbo",
            "name": "#__banners_banners_options"
        },
        "select": [
            "bbo.id",
            "bbo.option_id",
            "bbo_8097.title AS option_id_title",
            "bbo.banner_id",
            "bb.title AS banner_id_title",
            "bbo.created_by",
            "uu.name AS created_by_name",
            "bbo.date_created",
            "bbo.modified_by",
            "uu_3800.name AS modified_by_name",
            "bbo.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Banners\\Banners\\Options\\Code\\Tables\\Options",
                "alias": "bbo_8097",
                "name": "#__banners_banners_options",
                "on": "bbo.option_id = bbo_8097.id"
            },
            {
                "table": "Banners\\Banners\\Code\\Tables\\Banners",
                "alias": "bb",
                "name": "#__banners_banners",
                "on": "bbo.banner_id = bb.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "bbo.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_3800",
                "name": "#__users_users",
                "on": "bbo.modified_by = uu_3800.id"
            }
        ]
    }
