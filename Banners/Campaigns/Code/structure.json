    "query": {
        "from": {
            "table": "Banners\\Banners\\Campaigns\\Code\\Tables\\Campaigns",
            "alias": "bbc",
            "name": "#__banners_banners_campaigns"
        },
        "select": [
            "bbc.id",
            "bbc.banner_id",
            "bb.title AS banner_id_title",
            "bbc.campaign_id",
            "bc.title AS campaign_id_title",
            "bbc.created_by",
            "uu.name AS created_by_name",
            "bbc.date_created",
            "bbc.modified_by",
            "uu_5074.name AS modified_by_name",
            "bbc.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Banners\\Banners\\Code\\Tables\\Banners",
                "alias": "bb",
                "name": "#__banners_banners",
                "on": "bbc.banner_id = bb.id"
            },
            {
                "table": "Banners\\Campaigns\\Code\\Tables\\Campaigns",
                "alias": "bc",
                "name": "#__banners_campaigns",
                "on": "bbc.campaign_id = bc.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "bbc.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_5074",
                "name": "#__users_users",
                "on": "bbc.modified_by = uu_5074.id"
            }
        ]
