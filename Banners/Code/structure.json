    "query": {
        "from": {
            "table": "Banners\\Banners\\Code\\Tables\\Banners",
            "alias": "bb",
            "name": "#__banners_banners"
        },
        "select": [
            "bb.id",
            "bb.title",
            "bb.description",
            "bb.image",
            "mm.title AS image_title",
            "mm.file AS image_file",
            "bb.script",
            "bb.target_url",
            "bb.days",
            "bb.actions",
            "bb.points",
            "bb.package_id",
            "bp.title AS package_id_title",
            "bp.zone_id AS package_id_zone_id",
            "bp.option_id AS package_id_option_id",
            "bb.published",
            "bb.featured",
            "bb.created_by",
            "uu.name AS created_by_name",
            "bb.date_created",
            "bb.modified_by",
            "uu_6471.name AS modified_by_name",
            "bb.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "bb.image = mm.id"
            },
            {
                "table": "Banners\\Packages\\Code\\Tables\\Packages",
                "alias": "bp",
                "name": "#__banners_packages",
                "on": "bb.package_id = bp.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "bb.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_6471",
                "name": "#__users_users",
                "on": "bb.modified_by = uu_6471.id"
            }
        ]
    }
