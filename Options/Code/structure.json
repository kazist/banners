    "query": {
        "from": {
            "table": "Banners\\Options\\Code\\Tables\\Options",
            "alias": "bo",
            "name": "#__banners_options"
        },
        "select": [
            "bo.id",
            "bo.title",
            "bo.description",
            "bo.point_charged",
            "bo.published",
            "bo.created_by",
            "uu.name AS created_by_name",
            "bo.date_created",
            "bo.modified_by",
            "uu_1935.name AS modified_by_name",
            "bo.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "bo.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_1935",
                "name": "#__users_users",
                "on": "bo.modified_by = uu_1935.id"
            }
        ]
    }
