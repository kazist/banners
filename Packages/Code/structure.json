    "query": {
        "from": {
            "table": "Banners\\Packages\\Code\\Tables\\Packages",
            "alias": "bp",
            "name": "#__banners_packages"
        },
        "select": [
            "bp.id",
            "bp.title",
            "bp.zone_id",
            "bz.title AS zone_id_title",
            "bp.option_id",
            "bo.title AS option_id_title",
            "bp.point_charged",
            "bp.created_by",
            "uu.name AS created_by_name",
            "bp.date_created",
            "bp.modified_by",
            "uu_5710.name AS modified_by_name",
            "bp.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Banners\\Zones\\Code\\Tables\\Zones",
                "alias": "bz",
                "name": "#__banners_zones",
                "on": "bp.zone_id = bz.id"
            },
            {
                "table": "Banners\\Options\\Code\\Tables\\Options",
                "alias": "bo",
                "name": "#__banners_options",
                "on": "bp.option_id = bo.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "bp.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_5710",
                "name": "#__users_users",
                "on": "bp.modified_by = uu_5710.id"
            }
        ]
    }
